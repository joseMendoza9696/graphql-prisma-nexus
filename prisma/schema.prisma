// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE")
//  url      = "postgresql://postgres:mateito2010@localhost:5432/myApp"
}

generator client {
  provider = "prisma-client-js"
}

model Post {
    id          Int     @id @default(autoincrement())
    title       String
    body        String
    published   Boolean
}
